//Write a program to print whether a number is even or odd, also take input.


import java.util.*;

class Main {
	public static void main(String args[]) {
		// Your code goes here
		Scanner sc = new Scanner(System.in);

		int n = sc.nextInt();

		if(n%2==0){
			System.out.println("Even");
		}else{
			System.out.println("Odd");
		}
	}
}
